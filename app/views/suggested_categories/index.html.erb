<p id="notice"><%= notice %></p>

<h1>Suggested Categories</h1>

<table>
  <thead>
    <tr>
      <th>Category</th>
      <th>Approved</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @suggested_categories.each do |suggested_category| %>
      <tr>
        <td><%= suggested_category.category %></td>
        <td><%= suggested_category.approved %></td>
        <td><%= link_to 'Show', suggested_category %></td>
        <td><%= link_to 'Edit', edit_suggested_category_path(suggested_category) %></td>
        <td><%= link_to 'Destroy', suggested_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Suggested Category', new_suggested_category_path %>
